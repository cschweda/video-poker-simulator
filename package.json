{
  "name": "video-poker-simulator",
  "version": "1.0.0",
  "description": "Professional video poker simulator with optimal strategy analysis and comprehensive statistics",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .js,.ts,.vue --fix",
    "format": "prettier --write src/**/*.{js,ts,css,html}",
    "serve": "http-server dist -p 8080",
    "deploy": "yarn build && yarn serve"
  },
  "keywords": [
    "video-poker",
    "poker-simulator",
    "jacks-or-better",
    "casino-games",
    "gambling-simulation",
    "optimal-strategy",
    "rtp-analysis"
  ],
  "author": "Chris Schweda <cschweda@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cschweda/video-poker-simulator.git"
  },
  "bugs": {
    "url": "https://github.com/cschweda/video-poker-simulator/issues"
  },
  "homepage": "https://github.com/cschweda/video-poker-simulator#readme",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "http-server": "^14.1.1",
    "jsdom": "^23.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "chart.js": "^4.4.0",
    "lodash-es": "^4.17.21"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
